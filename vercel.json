{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/travner-web-frontend/browser",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://travner-web-backend-production.up.railway.app/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}